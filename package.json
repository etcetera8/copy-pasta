{
  "name": "copy-pasta",
  "productName": "Copy Pasta",
  "version": "1.0.0",
  "description": "A clipboard history application",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "contributors": [
    {
      "name": "Svet",
      "email": "svet.93@gmail.com"
    },
    {
      "name": "Parker",
      "email": "parker.t.lindley@gmail.com"
    }
  ],
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "pasta"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO"
          },
          "platforms": [
            "darwin"
          ]
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer/App.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ],
      "electronRebuildConfig": {
        "extraModules": [
          "robotjs"
        ],
        "force": true,
        "types": [
          "prod",
          "optional",
          "dev"
        ]
      }
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-dmg": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "6.0.0-beta.50",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/dateformat": "^3.0.1",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.0.0",
    "electron": "8.2.0",
    "electron-rebuild": "^1.10.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.0",
    "url-loader": "^4.0.0"
  },
  "dependencies": {
    "dateformat": "^3.0.3",
    "electron-clipboard-extended": "^1.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "mobx": "^5.15.4",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^6.1.8",
    "react": "^16.13.1",
    "react-debounce-input": "^3.2.2",
    "react-dom": "^16.13.1",
    "robotjs": "^0.6.0"
  }
}
